{
  "name": "Fruumo",
  "manifest_version":2,
  "version": "1.0.0",
  "offline_enabled":true,
  "background": {
    "scripts": ["./pages/background/onInstall.js", "./pages/background/wallpaper.js", "./pages/background/weather.js","./pages/background/screenshot.js"],
    "persistent": false
  },
  "options_page": "pages/settings/index.html",
  "chrome_url_overrides": {
      "newtab": "./pages/index/index.html"
  },
  "icons": {
    "128": "./images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "permissions":["tabs","webNavigation","management","history", "<all_urls>", "topSites", "bookmarks", "downloads","storage","unlimitedStorage","background","alarms"]
}