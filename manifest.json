{
  "name": "Fruumo - New Tab Page",
  "short_name":"Fruumo",
  "manifest_version":2,
  "version": "1.0.2",
  "offline_enabled":true,
  "background": {
    "scripts": ["./pages/background/browserAction.js", "./pages/background/onInstall.js", "./pages/background/wallpaper.js", "./pages/background/weather.js","./pages/background/screenshot.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'sha256-hlFrDEP6oHR9bqG8BbDuam6eZxvBUV8qhHdGISNsiLk='; object-src 'self'",
  "options_ui": {
    "page": "pages/settings/index.html",
    "chrome_style": true
  },
  "chrome_url_overrides": {
      "newtab": "./pages/index/index.html"
  },
  "icons": {
    "128": "./images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "permissions":["chrome://favicon/*", "<all_urls>", "https://ssl.google-analytics.com/", "https://www.google.com/s2/favicons","http://suggestqueries.google.com/complete/searcch","tabs","webNavigation","sessions","management","history", "topSites", "bookmarks", "downloads","storage","unlimitedStorage","background","alarms"]
}